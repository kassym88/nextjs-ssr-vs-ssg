{
  "extends": [
    "stylelint-config-recommended-scss",
    "stylelint-config-standard",
    "stylelint-config-rational-order"
  ],
  "plugins": [
    "stylelint-csstree-validator"
  ],
  "rules": {
    "no-missing-end-of-source-newline": null,
    "at-rule-no-unknown": [true, {
      "ignoreAtRules": [
        "function",
        "if",
        "each",
        "include",
        "mixin",
        "return",
        "warn"
      ]
    }]
  }
}
